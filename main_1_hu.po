msgid "Log on as well"
msgstr "Jelentkezzen be is"

#: main_1.cpp:96539
msgid "Log on as guest"
msgstr "Bejelentkezés vendégként"

#: main_1.cpp:96549
msgid "As registered user"
msgstr "Regisztrált felhasználóként"

#: main_1.cpp:96569
msgid "User name :"
msgstr "Felhasználónév:"

#: main_1.cpp:96597
msgid "Password :"
msgstr "Jelszó:"

