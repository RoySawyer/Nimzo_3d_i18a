msgstr "Masuk sebagai tamu"

#: main_1.cpp:96549
msgid "As registered user"
msgstr "Sebagai pengguna terdaftar"

#: main_1.cpp:96569
msgid "User name :"
msgstr "Nama belakang :"

#: main_1.cpp:96597
msgid "Password :"
msgstr "Kata Sandi :"

